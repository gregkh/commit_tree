      /* SPDX-License-Identifier: (LGPL-2.1 OR BSD-2-Clause) */
      /* THIS FILE IS AUTOGENERATED! */
    
      struct a_bpf {
            struct bpf_loader_ctx ctx;
      Segmentation fault (core dumped)
    
